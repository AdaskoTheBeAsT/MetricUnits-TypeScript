{
  "name": "metric-units",
  "$schema": "../../node_modules/nx/schemas/project-schema.json",
  "sourceRoot": "libs/metric-units/src",
  "projectType": "library",
  "tags": [],
  "targets": {
    "build": {
      "executor": "@nx/rollup:rollup",
      "options": {
        "outputPath": "dist/libs/metric-units",
        "main": "libs/metric-units/src/index.ts",
        "additionalEntryPoints": [
          "libs/metric-units/src/air-pressure.ts",
          "libs/metric-units/src/constants.ts",
          "libs/metric-units/src/enthalpy.ts",
          "libs/metric-units/src/enthalpy-unit.ts",
          "libs/metric-units/src/enum-utils.ts",
          "libs/metric-units/src/flow-rate.ts",
          "libs/metric-units/src/flow-rate-unit.ts",
          "libs/metric-units/src/height.ts",
          "libs/metric-units/src/height-unit.ts",
          "libs/metric-units/src/power.ts",
          "libs/metric-units/src/power-unit.ts",
          "libs/metric-units/src/pressure-unit.ts",
          "libs/metric-units/src/specific-humidity.ts",
          "libs/metric-units/src/specific-humidity-unit.ts",
          "libs/metric-units/src/temperature.ts",
          "libs/metric-units/src/temperature-unit.ts",
          "libs/metric-units/src/validation.ts",
          "libs/metric-units/src/velocity.ts",
          "libs/metric-units/src/velocity-unit.ts"
        ],
        "tsConfig": "libs/metric-units/tsconfig.lib.json",
        "assets": [],
        "format": ["esm", "cjs"],
        "generateExportsField": true,
        "compiler": "babel"
      }
    },
    "publish": {
      "command": "node tools/scripts/publish.mjs metric-units {args.ver} {args.tag}",
      "dependsOn": ["build"]
    },
    "lint": {
      "executor": "@nx/eslint:lint",
      "outputs": ["{options.outputFile}"]
    },
    "test": {
      "executor": "@nx/jest:jest",
      "outputs": ["{workspaceRoot}/coverage/{projectRoot}"],
      "options": {
        "jestConfig": "libs/metric-units/jest.config.ts"
      }
    }
  }
}
